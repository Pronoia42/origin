{
    "name": "@energyweb/origin-organization-irec-api-react-query-client",
    "version": "0.1.0",
    "description": "React Query Client library interacting with the Exchange",
    "homepage": "https://github.com/energywebfoundation/origin/tree/master/packages/api-clients/react-query/exchange#readme",
    "author": "EnergyWeb",
    "license": "GPL-3.0-or-later",
    "main": "dist/js/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/energywebfoundation/origin.git"
    },
    "bugs": {
        "url": "https://github.com/energywebfoundation/origin/issues"
    },
    "scripts": {
        "build": "yarn clean && yarn build:ts",
        "build:ts": "yarn build:client 1>/dev/null 2>/dev/null && tsc -b tsconfig.json",
        "build:client": "yarn client:generate:schema && yarn client:generate",
        "clean": "rm -rf dist dist-shakeable && yarn client:clean",
        "client:generate": "rm -rf src/client && orval && barrelsby -d src/client",
        "client:generate:schema": "ts-node src/generateSchema.ts",
        "client:clean": "rm -rf src/client src/schema.yaml"
    },
    "dependencies": {
        "axios": "0.21.1",
        "react-query": "3.15.1"
    },
    "devDependencies": {
        "typescript": "4.1.5",
        "@energyweb/origin-organization-irec-api": "1.4.0",
        "@nestjs/swagger": "4.8.0",
        "@nestjs/testing": "7.6.15",
        "@nestjs/typeorm": "7.1.5",
        "json-to-pretty-yaml": "1.2.2",
        "prettier": "2.2.1",
        "ts-node": "9.1.1",
        "@types/mocha": "8.2.2",
        "@types/node": "14.14.37",
        "mocha": "8.3.2",
        "typeorm": "0.2.34",
        "@nestjs/common": "7.6.15",
        "@nestjs/core": "7.6.15",
        "@types/supertest": "2.0.10",
        "supertest": "6.1.3",
        "supertest-capture-error": "1.0.0",
        "orval": "5.0.0-alpha.9",
        "barrelsby": "2.2.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "files": [
        "dist"
    ]
}
